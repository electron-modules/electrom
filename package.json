{
  "name": "electrom",
  "version": "17.0.0",
  "description": "Electrom is a resource management solution for Electron applications, which is convenient for performance management and friendly debugging of multiple windows",
  "keywords": [
    "electron",
    "electrom",
    "ps"
  ],
  "files": [
    "lib/**/*.js",
    "lib/coverage/reporter.html",
    "lib/monitor/reporter.template.html",
    "src/**/*.jsx",
    "src/**/*.less"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/electron-modules/electrom.git"
  },
  "dependencies": {
    "debug": "4.1.1",
    "filesize": "^6.1.0",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "mkdirp": "0.5.1",
    "runscript": "^1.5.0"
  },
  "devDependencies": {
    "@applint/spec": "^1.2.0",
    "@types/lodash": "^4.14.181",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "antd": "^4.9.4",
    "classnames": "^2.2.6",
    "concurrently": "^5.3.0",
    "css-loader": "^0.28.8",
    "electron": "17",
    "electron-windows": "17",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-egg": "^5.1.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-react": "^7.14.3",
    "git-contributor": "*",
    "husky": "*",
    "less": "3.11.3",
    "less-loader": "^6.2.0",
    "mocha": "*",
    "monitor.js": "*",
    "nyc": "*",
    "prop-types": "^15.7.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "style-loader": "^1.1.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "5.69.1",
    "webpack-cli": "4.9.0",
    "webpack-dev-server": "4.7.4"
  },
  "scripts": {
    "build": "webpack",
    "dev": "concurrently 'NODE_ENV=development webpack-dev-server' 'electron ./example/index.js --disable-gpu'",
    "dev:web": "NODE_ENV=development webpack-dev-server",
    "test": "nyc --reporter=lcov --reporter=text mocha --recursive",
    "lint": "eslint . --fix",
    "contributor": "git-contributor"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "license": "MIT"
}
