{
  "name": "electrom",
  "version": "0.3.0",
  "description": "Electrom is a resource management solution for Electron applications, which is convenient for performance management and friendly debugging of multiple windows",
  "keywords": [
    "electron",
    "electrom",
    "ps"
  ],
  "bin": {
    "electrom": "./bin/electrom.js"
  },
  "files": [
    "lib/**/*.js",
    "lib/coverage/reporter.html",
    "lib/monitor/reporter.template.html",
    "src/**/*.jsx",
    "src/**/*.less"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/electron-modules/electrom.git"
  },
  "dependencies": {
    "commander": "^6.2.0",
    "debug": "4.1.1",
    "filesize": "^6.1.0",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "mkdirp": "0.5.1",
    "runscript": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.7.4",
    "antd": "^4.9.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "concurrently": "^5.3.0",
    "css-loader": "^0.28.8",
    "electron": "11.3.0",
    "electron-windows": "^0.4.0",
    "eslint": "^6.4.0",
    "eslint-config-egg": "^5.1.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.14.3",
    "git-contributor": "*",
    "husky": "*",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "mocha": "*",
    "monitor.js": "*",
    "nyc": "*",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "style-loader": "^0.19.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "scripts": {
    "dev": "concurrently 'NODE_ENV=development webpack-dev-server' 'electron ./example/index.js'",
    "dev:web": "NODE_ENV=development webpack-dev-server",
    "test": "nyc --reporter=lcov --reporter=text mocha --recursive",
    "lint": "eslint . --fix",
    "contributor": "git-contributor"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "license": "MIT"
}
